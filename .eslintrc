{
    "rules": {
        "indent": [
            2,
            2
        ],
        "quotes": [
            2,
            "double"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-unused-vars": [
            2,
            { "args": "all", "argsIgnorePattern": "^_" }
        ]
    },
    "env": {
        "es6": true,
        "browser": true,
        "mocha": true,
        "jquery": true
    },
    "extends": "eslint:recommended",
    "ecmaFeatures": {
        "jsx": true,
        "modules": true
    }
}
